<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.VBox?>
<fx:root type="javafx.scene.layout.VBox" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml"
         stylesheets="@../css/base.css"
         fx:controller="us.nagro.august.caseapp.elements.NewKeyStoreDialog">
    <Label>KeyStore Name</Label>
    <TextField fx:id="name"/>

    <Label>Master Password</Label>
    <Label>(at least 8 characters)</Label>
    <PasswordField fx:id="pass"/>

    <Label>Confirm Master Password</Label>
    <PasswordField fx:id="confirmPass"/>

    <Label>Description (optional)</Label>
    <TextField fx:id="description"/>
</fx:root>
